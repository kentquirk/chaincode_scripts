VAR event = EVENT_TRANSFERANDLOCK
VAR qty = 0.00000001, 0.002, 600, 40000, 500000
VAR rate = 0.005
VAR minfee = 0.005
VAR maxfee = 0.5
VAR extra = 0.005
VAR ndau_to_napu = 100000000

BEGIN_TEMPLATE
load ../tx_fees.chbin

; Transfer and Lock - 0.05% of amount, minimum 0.005 ndau, maximum 0.5 ndau, plus 0.005 ndau

clear
push 0      ; size of transaction - unimportant
push { TX_QUANTITY:  {-{ int($qty * $ndau_to_napu) }-} }
event $event
run
expect {-{ int ((max (min ($qty * $rate, $maxfee), $minfee) + $extra) * $ndau_to_napu) }-}

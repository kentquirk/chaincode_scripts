SVAR event = EVENT_DEFAULT
VAR fee_ndau = 5000
VAR ndau_to_napu = 100000000

BEGIN_TEMPLATE
load ../tx_fees.chbin

; All transactions with a default fee of 5000 ndau

clear
push 0               ; size doesn't matter
push { TX_VALUE: 1 } ; transaction contents don't matter
event $event
run
expect {-{int ($fee_ndau * $ndau_to_napu)}-}

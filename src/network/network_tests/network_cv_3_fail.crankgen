VAR event = EVENT_CHANGEVALIDATION, EVENT_COMMANDVALIDATORCHANGE
VAR sigs = 0b_000_111,0b_111_000,0b_011_100,0b_011_010,0b_110_001,0b_101_001,0b_111_010

BEGIN_TEMPLATE
load ../network.chbin

;===== EVENT_CHANGEVALIDATION =====
;   ChangeValidation: 2 ndau Network signatures and 1 pre-BPC signature

; expect a failure for 3 or 4 signatures as long as there's no more than 1 network sig

clear
push 0
push { TX_QUANTITY: nd100 }
push $sigs
event $event
run fail

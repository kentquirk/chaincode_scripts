SVAR event = EVENT_CHANGEVALIDATION, EVENT_COMMANDVALIDATORCHANGE
SVAR sigs = 0b_000_011,0b_000_110,0b_001_100,0b_011_000,0b_110_000,0b_100_001

BEGIN_TEMPLATE
load ../network.chbin

;===== EVENT_CHANGEVALIDATION =====

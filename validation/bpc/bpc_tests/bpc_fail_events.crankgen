VAR event = EVENT_TRANSFER, EVENT_RELEASEFROMENDOWMENT, EVENT_CHANGERECOURSEPERIOD, EVENT_DELEGATE, EVENT_CREDITEAI, EVENT_LOCK, EVENT_NOTIFY, EVENT_SETREWARDSDESTINATION, EVENT_SETVALIDATION, EVENT_STAKE, EVENT_REGISTERNODE, EVENT_NOMINATENODEREWARD, EVENT_CLAIMNODEREWARD, EVENT_TRANSFERANDLOCK, EVENT_COMMANDVALIDATORCHANGE
VAR bpc = 111

BEGIN_TEMPLATE
load ../bpc.chbin

; - Permitted Transactions: ChangeValidation, SetSysvar
; - Validation Keys: 3 pre-BPC Keys
; - Validation Rules:
;     - ChangeValidation: 2 signatures
;     - SetSysvar: 2 signatures

; all non-specified events should fail even with 3 signatures
clear
push 0
push { TX_QUANTITY: nd100 }
push 0b_${bpc}
event $event
run fail
